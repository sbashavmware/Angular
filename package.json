    "karma-junit-reporter": "^1.2.0",
    "karma-mocha-reporter": "~2.2.1",
    "karma-remap-istanbul": "~0.6.0",
    "ng-apimock": "^1.4.6",
    "npm-run-all": "^4.1.3",
    "protractor": "~5.1.2",
    "ts-node": "^3.0.4",
    "tslint": "^5.3.2",
    "typescript": "^2.4.2",
    "typings": "^1.4.0",
